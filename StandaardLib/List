let list_from = i -> {
  item: i,
  node: ()
}

let list_prepend = i -> l -> {
  item: i,
  node: l
}

let rec list_map = f -> l -> {
  item: f(l.item),
  node: l.node == () ? () : list_map(f)(l.node) 
}

let rec list_count_i = i -> l -> l.node == () ? i : list_count_i(i + 1)(l.node)

let list_count = l -> list_count_i(1)(l)
